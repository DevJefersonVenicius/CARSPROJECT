generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Users { 
  id Int @id @default(autoincrement())
  email String
  cpf String? @unique // Cpf pode ficar nulo no banco de dados
  password String
  nameUser String
  @@map("users")
}

model Cars {
  id Int @id @default(autoincrement())
  nameCar String
  brand String
  model String
  version String
  color String
  description String
  accessories String
  location String
  phone String
  year String
  km String
  price String?
  images String?
  @@map("cars")
}
